{
  "name": "@principal-ai/quality-lens-registry",
  "version": "0.1.0",
  "description": "Centralized registry of quality lens metadata for the Principal AI quality toolchain",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "quality",
    "linting",
    "registry",
    "eslint",
    "biome",
    "typescript",
    "python",
    "ruff"
  ],
  "author": "Principal AI",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/principal-ai/codebase-quality-lens-registry.git"
  }
}
